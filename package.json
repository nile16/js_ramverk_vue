{
  "name": "me-vue",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/http-server/bin/http-server -s -p 8082 dist",
    "stop": "killall node",
    "pretest": "npm run build; npm run start &",
    "test": "mocha test/mocha.js; npm run lint",
    "posttest": "npm stop",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && rsync -av dist/* deploy@nile16.me:/var/www/nile16.me/html/"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "geckodriver": "^1.19.1",
    "http-server": "^0.11.1",
    "mocha": "^4.1.0",
    "raw-loader": "^3.1.0",
    "selenium-webdriver": "^3.6.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "## Project setup ``` npm install ```",
  "main": "babel.config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nile16/js_ramverk_vue.git"
  },
  "author": "Nils Leandersson (nile16)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nile16/js_ramverk_vue/issues"
  },
  "homepage": "https://github.com/nile16/js_ramverk_vue#readme"
}
